! scrollback
URxvt.scrollBar: false
URxvt.saveLines: 100000
URxvt.scrollTtyOutput: false
URxvt.scrollTtyKeypress: true
URxvt.scrollWithBuffer: true

!font
!URxvt.font: xft:Inconsolata Nerd Font:size=11, xft:mplus Nerd Font:size=10:style=regular
URxvt.font: xft:DejaVu Sans Mono:style=Book:pixelsize=17,\
            xft:Inconsolata Nerd Font:size=11:style=regular,\
            xft:M+1 Nerd Font:size=10:style=regular

!needed to fix font spacing
URxvt*letterSpace: -1
URxvt.intensityStyles: false
URxvt.internalBorder: 8

URxvt.iso14755: false

!fake transparancy
!Rxvt*transparent: true
!URxvt*shading: 30

!real transparancy
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800


!extensions
URxvt.perl-ext-common: selection-to-clipboard

!pywal
!#include "/home/leuf/.cache/wal/colors.Xresources"
